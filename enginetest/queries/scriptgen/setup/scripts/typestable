exec
CREATE TABLE `typestable` (
  `id` bigint NOT NULL,
  `i8` tinyint,
  `i16` smallint,
  `i32` int,
  `i64` bigint,
  `u8` tinyint unsigned,
  `u16` smallint unsigned,
  `u32` int unsigned,
  `u64` bigint unsigned,
  `f32` float,
  `f64` double,
  `ti` timestamp,
  `da` date,
  `te` text,
  `bo` tinyint,
  `js` json,
  `bl` blob,
  PRIMARY KEY (`id`)
)
----

