CREATE TABLE `people` (
  `dob` date NOT NULL,
  `first_name` text NOT NULL,
  `last_name` text NOT NULL,
  `middle_name` text NOT NULL,
  `height_inches` bigint NOT NULL,
  `gender` bigint NOT NULL,
  PRIMARY KEY (`dob`,`first_name`,`last_name`,`middle_name`)
)
----

