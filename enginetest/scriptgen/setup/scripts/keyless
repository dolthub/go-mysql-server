
exec
CREATE TABLE `unique_keyless` (
  `c0` bigint,
  `c1` bigint
)
----

exec
insert into unique_keyless values
    (0,0),
    (1,1),
    (2,2)
----

exec
CREATE TABLE `keyless` (
  `c0` bigint,
  `c1` bigint
)
----

exec
insert into keyless values
    (0,0),
    (1,1),
    (1,1),
    (2,2)
----

exec
CREATE INDEX keyless_c0_idx ON keyless (c0);
----

exec
CREATE UNIQUE INDEX keyless_c0_c1_idx ON keyless (c0, c1);
----

exec
CREATE INDEX keyless_c1_c0_odx ON keyless (c1, c0);
----